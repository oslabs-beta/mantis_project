{
  "name": "mantis",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:client": "vite build",
    "build:server": "tsc --outDir dist --project tsconfig.server.json",
    "build": "npm run build:client && npm run build:server",
    "dev:client": "vite",
    "dev:server": "nodemon --exec npx tsx server/server.ts",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "start": "node dist/server.js",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@influxdata/influxdb-client": "^1.35.0",
    "axios": "^1.7.9",
    "concurrently": "9.1.2",
    "express": "^4.21.2",
    "influxdb": "^0.0.1",
    "mongoose": "^8.10.0",
    "moongose": "1.0.0",
    "prom-client": "15.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "ts-node": "^10.9.2",
    "tsx": "4.19.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
