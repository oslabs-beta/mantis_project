    "bcrypt": "5.1.1",
    "bcryptjs": "2.4.3",
    "concurrently": "9.1.2",
    "cors": "2.8.5",
    "dotenv": "16.4.7",
    "express": "4.21.2",
    "influxdb": "^0.0.1",
    "jsonwebtoken": "9.0.2",
    "k6": "0.0.0",
    "mongoose": "8.10.0",
    "moongose": "1.0.0",
